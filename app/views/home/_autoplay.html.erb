<div class="hidden-xs">
        <div class="owl-carousel">
                <% @autoplayers.each do |imovel| %>
                        <div>
                        <%= link_to  imovel do %>
                        <%= image_tag imovel.capa.url(:lista), style: "width: 100%;margin-bottom: 10px;", class: "img-rounded"%>
                        <%end%>      
                        </div>    
                <%end%> 
        </div>
        <div style="margin-bottom: 20px;">
                <a class="btn btn-default play">Play</a> 
                <a class="btn btn-default stop">Stop</a>
        </div>
</div>